{
	"schema_version": 1,
	"quilt_loader": {
		"group": "com.example",
		"id": "modernui",
		"version": "${version}",
		"metadata": {
			"name": "LucidFont",
			"description": "Makes your font lucid! Text rendering codes of this mod are from ModernUI (https://github.com/BloCamLimb/ModernUI), thanks to BloCamLimb!",
			"contributors": {
				"calebxzhou": "aka Davickk"
			},
			"contact": {
				"homepage": "https://example.com/",
				"issues": "https://github.com/QuiltMC/quilt-template-mod/issues",
				"sources": "https://github.com/QuiltMC/quilt-template-mod"
			},
			"icon": "assets/example_mod/icon.png"
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
		"entrypoints": {
			"init": "calebxzhou.lucidfont.LucidFont"
		},
		"depends": [
			{
				"id": "quilt_loader",
				"versions": ">=0.17.6-"
			},
			{
				"id": "quilted_fabric_api",
				"versions": ">=5.0.0-"
			},
			{
				"id": "minecraft",
				"versions": ">=1.19.3"
			}
		]
	},
	"mixin": "mui.mixins.json"
}
